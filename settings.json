{
    "workbench.statusBar.feedback.visible": false,
    "workbench.activityBar.visible": false,
    "workbench.tree.indent": 12,
    "workbench.colorTheme": "Default Light+",
    "editor.fontFamily": "'Monokat 6x12', Consolas, 'Courier New', monospace",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.accessibilitySupport": "off",
    "editor.detectIndentation": false,
    "editor.emptySelectionClipboard": false,
    "editor.parameterHints.cycle": true,
    "debug.allowBreakpointsEverywhere": true,
    "debug.inlineValues": true,
    "window.autoDetectHighContrast": false,
    "window.menuBarVisibility": "visible",
    "workbench.sideBar.location": "right",
    "terminal.integrated.enableBell": true,
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.scrollback": 3000,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontWeightBold": "normal",
    "editor.renderControlCharacters": false,
    "editor.snippetSuggestions": "none",
    "files.hotExit": "off",
    "editor.showFoldingControls": "always",
    "editor.lineHeight": 12,
    // "problems.decorations.enabled": false,
    // "C_Cpp.errorSquiggles": "Disabled",
    "workbench.statusBar.visible": true,
    "git.path": "Z:\\Lander\\Unpacked Programs\\PortableGit\\bin\\git.exe",
    "grunt.autoDetect": "off",
    "gulp.autoDetect": "off",
    "jake.autoDetect": "off",
    "files.trimTrailingWhitespace": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "search.exclude": {
        "**/bower_components": false,
        "**/node_modules": false
    },
    "editor.suggestSelection": "first",
    "workbench.enableExperiments": false,
    "workbench.list.horizontalScrolling": true,
    "workbench.editor.highlightModifiedTabs": true,
    "window.restoreWindows": "none",
    "editor.trimAutoWhitespace": false,
    "explorer.confirmDelete": false,
    // "clangd.path": "Z:\\Lander\\LLVM\\bin\\clangd.exe",
    // "clangd.arguments": ["-background-index"],
    "C_Cpp.autoAddFileAssociations": false,
    "C_Cpp.autocomplete": "Disabled",
    "C_Cpp.configurationWarnings": "Disabled",
    "C_Cpp.dimInactiveRegions": false,
    "C_Cpp.errorSquiggles": "Disabled",
    "C_Cpp.formatting": "Disabled",
    "C_Cpp.intelliSenseEngine": "Disabled",
    "C_Cpp.clang_format_path": "Z:/Lander/msys2/mingw64/bin/clang-format.exe",
    "editor.autoSurround": "quotes",
    "surround.custom": {
        "parens": {
            "label": "()",
            "description": "()",
            "snippet": "($TM_SELECTED_TEXT)",
        },
        "braces": {
            "label": "{}",
            "description": "{}",
            "snippet": "{$TM_SELECTED_TEXT}",
        },
        "brackets": {
            "label": "[]",
            "description": "[]",
            "snippet": "[$TM_SELECTED_TEXT]",
        },
        "parens_lf": {
            "label": "(lf)",
            "description": "(lf)",
            "snippet": "(\n$TM_SELECTED_TEXT\n)",
        },
        "braces_lf": {
            "label": "{lf}",
            "description": "{lf}",
            "snippet": "{\n$TM_SELECTED_TEXT\n}",
        },
        "brackets_lf": {
            "label": "[lf]",
            "description": "[lf]",
            "snippet": "[\n$TM_SELECTED_TEXT\n]",
        },
    },
    "macros": {
        "surround_with_parens_l": ["surround.with.parens", "cursorLeft", "editor.action.jumpToBracket"],
        "surround_with_parens_r": ["surround.with.parens"],
        "surround_with_braces_l": ["surround.with.braces", "cursorLeft", "editor.action.jumpToBracket"],
        "surround_with_braces_r": ["surround.with.braces"],
        "surround_with_brackets_l": ["surround.with.brackets", "cursorLeft", "editor.action.jumpToBracket"],
        "surround_with_brackets_r": ["surround.with.brackets"],
        "surround_with_paren_block_l": ["surround.with.parens_lf", "cursorLeft", "editor.action.selectToBracket", "betterTab.indent", "cursorRight", "cursorLeft", "outdent", "editor.action.jumpToBracket", "outdent"],
        "surround_with_paren_block_r": ["macros.surround_with_paren_block_l", "editor.action.jumpToBracket", "cursorRight"],
        "surround_with_brace_block_l": ["surround.with.braces_lf", "cursorLeft", "editor.action.selectToBracket", "betterTab.indent", "cursorRight", "cursorLeft", "outdent", "editor.action.jumpToBracket", "outdent"],
        "surround_with_brace_block_r": ["macros.surround_with_brace_block_l", "editor.action.jumpToBracket", "cursorRight"],
        "surround_with_bracket_block_l": ["surround.with.brackets_lf", "cursorLeft", "editor.action.selectToBracket", "betterTab.indent", "cursorRight", "cursorLeft", "outdent", "editor.action.jumpToBracket", "outdent"],
        "surround_with_bracket_block_r": ["macros.surround_with_bracket_block_l", "editor.action.jumpToBracket", "cursorRight"],

        "close_param_hints_end" : ["closeParameterHints", "cursorEnd"],
        "close_suggestions_end" : ["hideSuggestWidget", "cursorEnd"],
    },
    "files.associations": {
        "*.json": "jsonc"
    },
    "git.enableSmartCommit": true,
    "editor.fontSize": 12,
    "breadcrumbs.enabled": false,
    "editor.suggest.filteredTypes": {
        "keyword": false
    },
    "editor.minimap.enabled": false,
    "debug.openExplorerOnEnd": true,
    "debug.showInStatusBar": "never",
    "debug.toolBarLocation": "docked",
    "debug.console.lineHeight": 12,
    "debug.console.fontFamily": "'Monokat 6x12', Consolas, 'Courier New', monospace",
    "debug.console.fontSize": 12,
    "files.exclude": {
        "obj/": true
    },
}